<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="io.cucumber.model.Test_result" table="test">
        <id name="ID" type="int">
            <column name="ID" />
<!--            <generator class="assigned"/>-->
        </id>

        <property name = "test_id" column = "test_id" type = "int"/>
        <property name = "result" column = "result" type = "int"/>
        <property name = "test_time" column = "test_time" type = "double"/>
        <property name = "date" column = "date" type = "java.util.Date"/>
        <property name = "last_step" column = "last_step" type = "java.lang.String"/>
    </class>
</hibernate-mapping>